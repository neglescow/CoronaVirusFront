A,GAAA,KAAA,QAAA,CAAA,IAAA,IAAA,KAAA,QAAA,CAAA,MAAA,GAAA,CAAA,CAAA;AAEA,UAAA,GAAA,IAAA,EAAA,EAAA,UAAA,GAAA,KAAA,CAAA,KACA,UAAA,GAAA,KAAA;AAEA,UAAA,KAAA,QAAA,CAAA,KAAA,IAAA,EAAA,EAAA,UAAA,GAAA,KAAA,CAAA,KACA,UAAA,GAAA,KAAA;AAEA,UAAA,aAAA,GAAA;AACA,QAAA,IAAA,EAAA,KAAA,QAAA,CAAA,IADA;AAEA,QAAA,QAAA,EAAA,KAAA,QAAA,CAAA,QAFA;AAGA,QAAA,QAAA,EAAA,KAAA,QAAA,CAAA,QAHA;AAIA,QAAA,IAAA,EAAA,KAAA,QAAA,CAAA,IAJA;AAKA,QAAA,MAAA,EAAA,KAAA,QAAA,CAAA,MALA;AAMA,QAAA,KAAA,EAAA,KAAA,QAAA,CAAA,KANA;AAOA,QAAA,UAAA,EAAA,KAAA,QAAA,CAAA,UAPA;AAQA,QAAA,SAAA,EAAA,UARA;AASA,QAAA,KAAA,EAAA;AATA,OAAA;AAWA,MAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,iCAAA;AACA,MAAA,QAAA,CAAA,MAAA;AAEA,MAAA,WAAA,CAAA,aAAA,CAAA,aAAA,EACA,IADA,CACA,UAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,QAAA,CAAA,EAAA,GAAA,IAAA,CAAA,IAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,QAAA;AACA,OAJA,EAIA,KAJA,CAIA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,OAAA;AACA,OANA;AAOA;AAjCA;AAhBA,CAAA","sourcesContent":["<template>\r\n    <div class=\"submit-form\">\r\n        <div class=\"form-group\">\r\n            <label for=\"nome\">Nome</label>\r\n            <input \r\n                type=\"text\" \r\n                class=\"form-control\" \r\n                id=\"nome\" \r\n                v-model=\"paciente.nome\" \r\n                name=\"nome\"\r\n            />\r\n        </div>\r\n        <div class=\"form-group\">\r\n            <label for=\"endereco\">Endereço</label>\r\n            <input \r\n                type=\"text\" \r\n                class=\"form-control\" \r\n                id=\"endereco\" \r\n                v-model=\"paciente.endereco\" \r\n                name=\"endereco\"\r\n            />\r\n        </div>\r\n        <div class=\"form-group\">\r\n            <label for=\"telefone\">Telefone</label>\r\n            <input \r\n                type=\"text\" \r\n                max=\"11\" \r\n                class=\"form-control\" \r\n                id=\"telefone\"\r\n                v-model=\"paciente.telefone\" \r\n                name=\"telefone\"\r\n            />\r\n        </div>\r\n        <div class=\"form-group\">\r\n            <label for=\"peso\">Peso</label>\r\n            <input \r\n                type=\"number\" \r\n                class=\"form-control\" \r\n                id=\"peso\" \r\n                v-model=\"paciente.peso\" \r\n                name=\"peso\"\r\n            />\r\n        </div>\r\n        <div class=\"form-group\">\r\n            <label for=\"altura\">Altura</label>\r\n            <input \r\n                type=\"number\" \r\n                class=\"form-control\" \r\n                id=\"altura\" \r\n                v-model=\"paciente.altura\" \r\n                name=\"altura\"\r\n            />\r\n        </div>\r\n        <div class=\"form-group\">\r\n            <label for=\"idade\">Idade</label>\r\n            <input \r\n                type=\"number\" \r\n                class=\"form-control\" \r\n                id=\"idade\" \r\n                v-model=\"paciente.idade\" \r\n                name=\"idade\"\r\n            />\r\n        </div>\r\n        <div class=\"form-group\">\r\n            <label for=\"prob_saude\">Complicações</label>\r\n            <input \r\n                type=\"text\" \r\n                class=\"form-control\" \r\n                id=\"prob_saude\" \r\n                v-model=\"paciente.prob_saude\" \r\n                name=\"prob_saude\"\r\n            />\r\n        </div>\r\n\r\n        <button @click=\"inserirPaciente\" class=\"btn btn-success\">\r\n            inserir Paciente\r\n        </button>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport PacientesWS from '../services/PacientesWS';\r\n    export default {\r\n        name: \"adicionarPaciente\",\r\n        data(){\r\n            return {\r\n                paciente: {\r\n                    id: \"\",\r\n                    nome: \"\",\r\n                    endereco: \"\",\r\n                    telefone: \"\",\r\n                    peso: 0,\r\n                    altura: 0,\r\n                    idade: 0,\r\n                    prob_saude: \"\"\r\n                }\r\n            }\r\n        },\r\n        methods: {\r\n            inserirPaciente(){\r\n                let verifObeso = \"\"\r\n                let verifIdoso = \"\"\r\n                let IMC = this.paciente.peso / (this.paciente.altura * 2)\r\n                \r\n                if(IMC <= 25) verifObeso = \"Não\"\r\n                else verifObeso = \"Sim\"\r\n\r\n                if(this.paciente.idade >= 55) verifIdoso = \"Sim\"\r\n                else verifIdoso = \"Não\"\r\n\r\n                let dadosPaciente = {\r\n                    nome: this.paciente.nome,\r\n                    endereco: this.paciente.endereco,\r\n                    telefone: this.paciente.telefone,\r\n                    peso: this.paciente.peso,\r\n                    altura: this.paciente.altura,\r\n                    idade: this.paciente.idade,\r\n                    prob_saude: this.paciente.prob_saude,\r\n                    obesidade: verifObeso,\r\n                    idoso: verifIdoso\r\n                }\r\n                window.location.replace(\"http://localhost:8081/pacientes\")\r\n                location.reload()\r\n\r\n                PacientesWS.criarPaciente(dadosPaciente)\r\n                    .then(resp => {\r\n                        this.paciente.id = resp.data.id;\r\n                        console.log(this.paciente);\r\n                    }).catch(error => {\r\n                        console.log(error.message);\r\n                    })\r\n            }\r\n        }\r\n    }\r\n</script>"],"sourceRoot":"src/components"}]}